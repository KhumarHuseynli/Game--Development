	if gs =="game" then
--	ubm()
	umap()
	uship()
	uplanet()
	ubullets()
	uasteroid()
 ulife()
	elseif gs=="start" then
	update_start()
	elseif gs =="win" then 
	update_win()
	elseif gs =="over" then 
	update_over()
	end
end

function _draw()
